             $("body").on("click", '.minus-btn, .plus-btn, .tt-btn-close, .tt-btn-delete-all', function (e) {
                 var $input = $(this).parent().find('input');
                 var count = parseInt($input.val(), 10) + parseInt(e.currentTarget.className === 'plus-btn' ? 1 : -1, 10);
                 var ProductRowId = $input[0].dataset["productid"];
                 var page = $input[0].dataset["page"];
                 if (count == -1 || isNaN(count))
                 {
                     Fn_AddProductAndRemove($input, 0);
                     $(this).closest('.tt-item-product-delete-' + ProductRowId).remove();
                     if (page == "master") {
                         $("#item-reviewProduct-" + ProductRowId).closest('.tt-item-product-delete-' + ProductRowId).remove();
                     }
                     if (page == "review") {
                         $("#item-masterProduct-" + ProductRowId).closest('.tt-item-product-delete-' + ProductRowId).remove();
                     }
                     if (page == "review-delete-all") {
                         $("#tblBodyTable").html("");
                        // $("#tblBodyTable").append(CleanHtmlTableReview);
                     }
                     $("#tblBodyTable").append(CleanHtmlTableReview);
                 } else {
                     if (count >= 1)
                     {
                         Fn_AddProductAndRemove($input, count);
                     }
                     else {
                         return;
                     }
                 }         
             });

             function Fn_AddProductAndRemove($input, count, page) {
                 //00 eliminar todo 
                 $input.val(count).change();
                 var productId = $input[0].dataset["productid"];
                 var quantity = $input.val();
                 if (productId > 0 || productId == 00) {
                     $("#tt-pageContent").append("<div class='loader_page'></div>");
                     var objData = {
                         ProductQuantity: quantity, ProductId: productId
                     };
                     var success = function (asw) {
                         try {
                             if (asw.d != null) {
                                 if (asw.d.Result == "Ok") {
                                     obj = $.parseJSON(asw.d.OrderHeader); 
                                     CalculateMaster(obj);
                                     if (obj.lstItemsCount[0].length == 0) {
                                         $("#lstCartView").append(NotProductCart);
                                         $("#idCantidadProductos").text(obj.lstItemsCount[0].length);
                                         $("#btnMasterGoPay").attr("hidden", true);
                                         $("#ContentPlaceHolder1_btnGeneratePedido").attr("hidden", true);
                                         //$("#tblCarrito").append(CleanHtmlTableReview)

                                     } else {
                                         $("#idCantidadProductos").text(obj.lstItemsCount[0].length);
                                         $("#btnMasterGoPay").removeAttr('hidden')
                                         $("#ContentPlaceHolder1_btnGeneratePedido").removeAttr("hidden");
                                     }
                                 }
                                 else {
                                     if (asw.d.Result == "NoOk") { }
                                 }
                             }
                         } catch (e) {
                             $("#tt-pageContent").find(".loader_page").remove();
                         }
                     };
                     var error = function (jqXHR, textStatus, errorThrown) {
                         $("#tt-pageContent").find(".loader_page").remove();
                     };
                     var complete = function () {
                         $("#tt-pageContent").find(".loader_page").remove();
                     };
                     fn_callmethodComplete("Review.aspx/CartProduct_UpdateQuantity", JSON.stringify({ data: objData }), success, error, complete);
                 }
		   }
             
             function CalculateMaster(ObjOrderHeader) {
                 if (ObjOrderHeader.Detail.ProductId == 0) {
                     $("#lstCartView").html("");
                     $("#idSubTotalRight").text("S/." + ObjOrderHeader.SubTotal.toFixed(2));
                     $("#idDeliveryTotalRight").text("S/." + ObjOrderHeader.DeliveryTotal.toFixed(2));
                     //$('#idSubTotalProduct_' + ObjOrderHeader.Detail.ProductId).text("S/." + ObjOrderHeader.Detail.Totalprice.toFixed(2));
                     $(".tt-cart-total-price").text("S/." + ObjOrderHeader.SubTotal.toFixed(2));
                     $("#idTotalRigth").text("S/." + ObjOrderHeader.Ordertotal.toFixed(2));  
                 } else {
                     $('#unitPriceProduct_' + ObjOrderHeader.Detail.ProductId).text("S/." + ObjOrderHeader.Detail.Product.UnitPrice.toFixed(2));
                     $('.tt-quantity_' + ObjOrderHeader.Detail.ProductId).text(ObjOrderHeader.Detail.Quantity + " X " + ObjOrderHeader.Detail.Product.UnitPrice);
                     $(".tt-cart-total-price").text("S/." + ObjOrderHeader.SubTotal.toFixed(2));
                     $("#idSubTotalRight").text("S/." + ObjOrderHeader.SubTotal.toFixed(2));
                     $("#idDeliveryTotalRight").text("S/." + ObjOrderHeader.DeliveryTotal.toFixed(2));
                     $('#idSubTotalProduct_' + ObjOrderHeader.Detail.ProductId).text("S/." + ObjOrderHeader.Detail.Totalprice.toFixed(2));
                     $("#idTotalRigth").text("S/." + ObjOrderHeader.Ordertotal.toFixed(2));  
                 }       
             }

             if ($("#<%=hfDataListProduct.ClientID%>").val() != "") {
                 Fn_ListProductsShopCartHeader($("#<%=hfDataListProduct.ClientID%>").val());
             } else {
                 $("#lstCartView").append(NotProductCart);
